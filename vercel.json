{
  "version": 2,
  "framework": "sveltekit",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.svelte-kit/output",
  "installCommand": "cd frontend && npm install",
  "regions": ["sin1"],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1",
      "continue": true
    },
    {
      "src": "/(.*\\.(js|css|svg|jpg|jpeg|png|gif|ico|woff|woff2)$)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/test\\.html",
      "dest": "/static/test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 