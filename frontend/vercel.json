{
  "version": 2,
  "framework": "sveltekit",
  "buildCommand": "npm run build",
  "outputDirectory": ".svelte-kit/output",
  "installCommand": "npm install",
  "regions": ["sin1"],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/fn"
    }
  ],
  "functions": {
    "fn": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 